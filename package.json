{
  "name": "chart-node-server",
  "version": "0.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "tsc",
    "start": "node out/index",
    "start:w": "nodemon out/index",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "docker": "docker build . -t node-chart-server",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "node out-test/test/all-tests",
    "cover": "nyc npm test"
  },
  "keywords": [
    "chart",
    "http",
    "rest",
    "api"
  ],
  "author": "Harald Niesche",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.6.1",
    "@hn3000/canvas-5-polyfill": "^0.1.5-hn2",
    "d3": "^6.1.1",
    "express": "^4.17.1",
    "full-icu": "^1.3.0"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.2",
    "nyc": "^15.1.0",
    "tsunit.external": "^2.1.1",
    "typescript": "^4.0.3"
  },
  "nyc": {
    "all": true,
    "excludeAfterRemap": false,
    "extension": [
      ".js",
      ".ts"
    ],
    "include": [
      "out-test/src/**",
      "src/*.ts"
    ],
    "exclude": [
      "out-test/test/**",
      "test/*.ts"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  }
}
