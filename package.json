{
  "name": "chart-node-server",
  "version": "0.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "tsc",
    "start": "node out/index",
    "prepare": "tsc",
    "tsc": "tsc",
    "predocker": "tsc",
    "docker": "docker build . -t node-chart-server",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "node out-test/test/all-tests",
    "cover": "nyc npm test"
  },
  "keywords": [
    "chart",
    "http",
    "rest",
    "api"
  ],
  "author": "Harald Niesche",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.6.0",
    "chart.js": "^2.9.2",
    "d3": "^5.12.0",
    "express": "^4.17.1",
    "full-icu": "^1.3.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.8.10",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.2",
    "typescript": "^3.6.4",
    "nyc": "^14.1.1",
    "tsunit.external": "^2.1.1"
  },
  "nyc": {
    "all": true,
    "excludeAfterRemap": false,
    "extension": [
      ".js",
      ".ts"
    ],
    "include": [
      "out-test/src/**",
      "src/*.ts"
    ],
    "exclude": [
      "test/**",
      "out-test/test/**"
    ],
    "reporter": [
      "text",
      "html"
    ]
  }
}
